{
	"name": "static",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"vite\" \"npm run watch:css\"",
		"build": "vite build",
		"preview": "vite preview",
		"build:css": "tailwindcss -i ./static/app.css -o ./src/styles/main.css",
		"watch:css": "tailwindcss -i ./static/app.css -o ./src/styles/main.css --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"concurrently": "^8.2.2",
		"svelte": "^4.2.7",
		"tailwindcss": "^3.4.3",
		"vite": "^5.2.11"
	},
	"type": "module",
	"dependencies": {
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.38"
	}
}
